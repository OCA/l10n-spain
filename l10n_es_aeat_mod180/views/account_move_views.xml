<?xml version="1.0" encoding="utf-8" ?>
<odoo>
    <record id="view_move_form_inherit_mod180" model="ir.ui.view">
        <field name="name">view.move.form.inherit.mod180</field>
        <field name="model">account.move</field>
        <field name="inherit_id" ref="account.view_move_form" />
        <field name="arch" type="xml">
            <xpath
                expr="//field[@name='line_ids']//tree/field[@name='debit']"
                position="before"
            >
                <field name="selectable_real_estate_ids" invisible="1" />
                <field
                    name="l10n_es_aeat_real_estate_id"
                    domain="[('id', 'in', selectable_real_estate_ids)]"
                />
            </xpath>
        </field>
    </record>

</odoo>
