<?xml version="1.0" encoding="utf-8"?>
<openerp>
    <data>

        <record id="l10n_es_sii_password_wizard_view" model="ir.ui.view">
            <field name="name">l10n.es.aeat.sii.password.wizard</field>
            <field name="model">l10n.es.aeat.sii.password</field>
            <field name="type">form</field>
            <field name="arch" type="xml">
                <form string="Insert Password">
                    <group colspan="4" col="4">
                        <field name="password"/>
                    </group>
                    <group colspan="4" col="4">
                        <button name="get_keys" type="object" string="Obtain Keys"/>
                    </group>
                </form>
            </field>
        </record>

    </data>
</openerp>
