	</simpleType>
	<!-- Cadena de 250 caracteres -->
	<simpleType name="TextMax250Type">
		<restriction base="string">
			<maxLength value="250"/>
		</restriction>
	</simpleType>
	<simpleType name="intPositiveType">
        <restriction base="int">
            <minInclusive value="1"/>
        </restriction>
    </simpleType>
	<simpleType name="int1to99Type">
		<restriction base="int">
			<minInclusive value="1"/>
			<maxInclusive value="99"/>
		</restriction>
	</simpleType>
	<simpleType name="intPorcentajeType">
		<restriction base="int">
			<minInclusive value="0"/>
			<maxInclusive value="100"/>
		</restriction>
	</simpleType>
	<simpleType name="decimalPorcentajeType">
		<restriction base="decimal">
			<minInclusive value="0"/>
			<maxInclusive value="100"/>
			<fractionDigits value="2"/>
		</restriction>
	</simpleType>
	<simpleType name="intOf3Digits">
		<restriction base="int">
			<pattern value="\d{1,3}"/>
		</restriction>
	</simpleType>
	<simpleType name="stringOf5Digits">
		<restriction base="string">
			<length value="5"/>
			<pattern value="[0-9]*">
			</pattern>
		</restriction>
	</simpleType>
	<simpleType name="stringMax2Digits">
		<restriction base="string">
			<maxLength value="2"/>
			<pattern value="[0-9]*">
			</pattern>
		</restriction>
	</simpleType>
	<simpleType name="stringMax5Digits">
		<restriction base="string">
			<maxLength value="5"/>
			<pattern value="[0-9]*">
			</pattern>
		</restriction>
	</simpleType>
	<simpleType name="stringMax7Digits">
		<restriction base="string">
			<maxLength value="7"/>
			<pattern value="[0-9]*">
			</pattern>
		</restriction>
	</simpleType>
	<simpleType name="Importe3.2Type">
		<restriction base="string">
			<pattern value="\d{1,3}(\.\d{0,2})?"/>
		</restriction>
	</simpleType>
	<simpleType name="ImporteSgn12.2Type">
		<restriction base="string">
			<pattern value="(\+|-)?\d{1,12}(\.\d{0,2})?"/>
		</restriction>
	</simpleType>
	<simpleType name="FechaType">
		<restriction base="string">
			<length value="10"/>
			<pattern value="\d{2,2}-\d{2,2}-\d{4,4}"/>
		</restriction>
	</simpleType>
	<simpleType name="HoraType">
		<restriction base="string">
			<pattern value="\d{2,2}:\d{2,2}:\d{2,2}"/>
		</restriction>
	</simpleType>
	<simpleType name="FechaConHoraType">
		<restriction base="string">
			<length value="19"/>
			<pattern value="\d{2,2}-\d{2,2}-\d{4,4} \d{2,2}:\d{2,2}:\d{2,2}"/>
		</restriction>
	</simpleType>
	<simpleType name="YearType">
		<annotation>
			<documentation xml:lang="es"> Año en formato YYYY </documentation>
		</annotation>
		<restriction base="int">
			<pattern value="\d{4,4}"/>
		</restriction>
	</simpleType>
	<simpleType name="NIFType">
		<annotation>
			<documentation xml:lang="es">NIF: Secuencia de 9 dígitos o letras mayúsculas </documentation>
		</annotation>
		<restriction base="string">
			<length value="9"/>
			<pattern
				value="(([a-z|A-Z]{1}\d{7}[a-z|A-Z]{1})|(\d{8}[a-z|A-Z]{1})|([a-z|A-Z]{1}\d{8}))"/>
		</restriction>
		<!-- ([A-Z]|\d){9} -->
	</simpleType>
</schema>
