msgstr "l10n_cat_account.wizard"

#. module: l10n_cat_account
#: view:l10n_cat_account.wizard:0
msgid "Translate"
msgstr "Traducir"

#. module: l10n_cat_account
#: model:ir.actions.act_window,name:l10n_cat_account.action_l10n_cat_account_wizard
#: view:l10n_cat_account.wizard:0
