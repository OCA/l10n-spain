<?xml version="1.0" encoding="utf-8" ?>
<odoo>
        <record id="view_base_document_layout_ecoembes_inherit" model="ir.ui.view">
            <field name="name">Document Layout with ecoembes</field>
            <field name="model">base.document.layout</field>
            <field name="inherit_id" ref="web.view_base_document_layout" />
            <field name="arch" type="xml">
                <xpath expr="//field[@name='company_id']" position="after">
                    <field name="ecoembes_inscription" invisible="1" />
                    <field name="ecoembes_partner_member" invisible="1" />
                </xpath>
            </field>
        </record>
</odoo>
