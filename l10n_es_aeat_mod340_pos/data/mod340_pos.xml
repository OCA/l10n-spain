<?xml version="1.0" encoding="UTF-8"?>
<odoo>
    <data>

        <function model="pos.config" name="_uncheck_group_by"/>

        <!-- 178-217     Identificación de la factura -->
        <record id="l10n_es_aeat_mod340.aeat_mod340_invoice_issued_export_line_21"
                model="aeat.model.export.config.line">
            <field name="sequence">21</field>
            <field name="export_config_id"
                   ref="l10n_es_aeat_mod340.aeat_mod340_invoice_issued_export_config"/>
            <field name="name">Identificación de la factura</field>
            <field name="expression">
                ${object.invoice_record_id.invoice_id.number if not object.invoice_record_id.simplified_invoice_ref else object.invoice_record_id.simplified_invoice_ref}
            </field>
            <field name="export_type">string</field>
            <field name="size">40</field>
            <field name="alignment">left</field>
        </record>

    </data>
</odoo>