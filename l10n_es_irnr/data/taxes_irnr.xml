<odoo>

<!-- Retenciones IRPF No residentes 24,75% -->

<record id="account_tax_template_s_irpfnr2475" model="account.tax.template">
    <field name="description">S_IRPFNR2475</field>
    <field name="type_tax_use">sale</field>
    <field name="account_id" ref="l10n_es.pgc_473_child"/>
    <field name="name">Retenciones a cuenta IRPF No residentes 24,75%</field>
    <field name="refund_account_id" ref="l10n_es.pgc_473_child"/>
    <field name="chart_template_id" ref="l10n_es.account_chart_template_common"/>
    <field name="amount" eval="-24.75"/>
    <field name="amount_type">percent</field>
</record>

<record id="account_tax_template_p_irpfnr2475p" model="account.tax.template">
    <field name="description">P_IRPFNR2475P</field>
