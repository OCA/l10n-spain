<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <record id="send_aeat_silicie_view" model="ir.ui.view">
        <field name="name">AEAT Send SILICIE</field>
        <field name="model">send.aeat.silicie.wizard</field>
        <field name="arch" type="xml">
            <form string="Send AEAT SILICIE">
                <span>Do you want to proceed?</span>
                <footer>
                    <button name="send_aeat_silicie" string="Send" type="object" default_focus="1" class="oe_highlight" />
                    <button string="Cancel" class="oe_link" special="cancel" />
                </footer>
            </form>
        </field>
    </record>
    <act_window id="send_aeat_silicie" name="Send SILICIE AEAT" res_model="send.aeat.silicie.wizard" src_model="stock.move" view_mode="form" target="new" key2="client_action_multi" />
</odoo>
