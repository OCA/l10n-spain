<?xml version="1.0" ?>
<odoo noupdate="1">
    <record id="face_ws_link" model="ir.config_parameter">
        <field name="key">facturae.faceb2b.ws</field>
        <field name="value">https://se-ws-faceb2b.redsara.es/sv1/invoice?wsdl</field>
    </record>

    <record model="ir.cron" id="ir_cron_faceb2b_receive">
        <field name="name">FaceB2B: Receive Invoices</field>
        <field name="interval_number">1</field>
        <field name="interval_type">hours</field>
        <field name="numbercall">-1</field>
        <field name="state">code</field>
        <field name="doall" eval="False" />
        <field name="model_id" ref="account.model_account_journal" />
        <field name="code">model._cron_facturae_faceb2b()</field>
    </record>
    <record model="ir.cron" id="ir_cron_faceb2b_request_to_cancel">
        <field name="name">FaceB2B: Requests to cancel invoices</field>
        <field name="interval_number">1</field>
        <field name="interval_type">hours</field>
        <field name="numbercall">-1</field>
        <field name="state">code</field>
        <field name="doall" eval="False" />
        <field name="model_id" ref="account.model_account_journal" />
        <field name="code">model._cron_facturae_cancel_faceb2b()</field>
    </record>
</odoo>
