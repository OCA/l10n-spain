<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <record id="mod_111_11" model="account.account.tag">
        <field name="name">mod111[11]</field>
        <field name="applicability">taxes</field>
        <field name="country_id" ref="base.es"/>
    </record>
