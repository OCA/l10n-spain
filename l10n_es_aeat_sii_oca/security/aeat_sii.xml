<?xml version="1.0" encoding="UTF-8" ?>
<odoo>
    <record id="ir_cron_trigger_sii_rule" model="ir.rule">
        <field name="name">Triggers AEAT SII visibility</field>
        <field name="model_id" ref="base.model_ir_cron_trigger" />
        <field
            name="domain_force"
            eval="[('cron_id', '=', ref('l10n_es_aeat_sii_oca.invoice_send_to_sii'))]"
        />
        <field name="groups" eval="[(4, ref('l10n_es_aeat.group_account_aeat'))]" />
    </record>
</odoo>
