<odoo>
    <record id="view_move_form" model="ir.ui.view">
        <field name="model">account.move</field>
        <field name="inherit_id" ref="account.view_move_form" />
        <field name="arch" type="xml">
            <xpath expr="//field[@name='ref'][last()]" position="after">
                <field name="thirdparty_invoice" attrs="{'invisible': 1}" />
                <field name="thirdparty_number" attrs="{'invisible': 1}" />
            </xpath>
            <xpath
                expr="//field[@name='invoice_line_ids']//field[@name='account_id']"
                position="after"
            >
                <field
                    name='l10n_es_aeat_real_estate_id'
                    optional='hide'
                    context="{'default_partner_id': partner_id}"
                />
            </xpath>
            <xpath
                expr="//field[@name='line_ids']//field[@name='account_id']"
                position="after"
            >
                <field
                    name='l10n_es_aeat_real_estate_id'
                    optional='hide'
                    context="{'default_partner_id': partner_id}"
                />
            </xpath>
        </field>
    </record>
</odoo>
